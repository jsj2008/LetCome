@implementation LGBFooterSection
-(void)awakeFromNib{
    NSMutableAttributedString *str = [[NSMutableAttributedString alloc]initWithString:self.lblLGBDesc.text];
    [str addAttribute:NSForegroundColorAttributeName value:[UIColor colorWithRed:10/255.0 green:130/255.0 blue:250/255.0 alpha:1.0] range:NSMakeRange(9, 3)];
    self.lblLGBDesc.attributedText = str;
}

@end

