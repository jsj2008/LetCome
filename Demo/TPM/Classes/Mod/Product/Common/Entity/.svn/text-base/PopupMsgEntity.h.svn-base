#import "QUEntity.h"

@interface PopupMsgEntity : QUEntity
@property (nonatomic,copy) NSString *ID;// 增量ID	int
@property (nonatomic,copy) NSString *sender_id;//	送信者ID uid	int
@property (nonatomic,copy) NSString *msg_id;//	消息编号	Int
@property (nonatomic,copy) NSString *msg_type;//	消息类型:0-公告 1-通知 2-活动 3-提醒	Int
@property (nonatomic,copy) NSString *msg_type_name;//	消息类型名称	String
@property (nonatomic,copy) NSString *message_title;//	消息标题	String
@property (nonatomic,copy) NSString *read_status;//	消息状态：0-未读，1-已读，2-删除	Int
@property (nonatomic,copy) NSString *create_time;//	消息发送时间	int
@property (nonatomic,copy) NSString *message;//	消息内容	string
@property (nonatomic,copy) NSString *popup;//	弹出类型 0：不弹 1：弹小窗 2： 弹全屏窗	int
@end
