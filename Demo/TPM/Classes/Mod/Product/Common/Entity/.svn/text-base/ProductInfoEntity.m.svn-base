#import "ProductInfoEntity.h"

@implementation ProductInfoEntity
-(NSString*)parseJson:(NSDictionary*)json{
    NSDictionary *jsonDict = (NSDictionary *)json;
    NSMutableDictionary *dict = [NSMutableDictionary dictionaryWithDictionary:jsonDict];

    NSDictionary *rootDict = nil;
    NSMutableDictionary *tempDict = [NSMutableDictionary dictionaryWithCapacity:2];
    
    [dict enumerateKeysAndObjectsUsingBlock:^(id key, NSDictionary *obj, BOOL *stop) {
        
        [tempDict addEntriesFromDictionary:obj];
        
    }];
    
    rootDict = tempDict;
    return [rootDict JSONString];
    
}
@end
