#import <UIKit/UIKit.h>

@protocol KeyBoardDelegate  <NSObject>

-(void)confirmFunc;
-(void)leftFunc;
-(void)rightFunc;
-(void)closeFunc;
-(void)textChangeTo:(NSString *)text;
@optional
-(void)textHasChanged;

@end





@interface STOKeyBoard : UIView

@property(retain,nonatomic)id<KeyBoardDelegate> keyBoardDelegate;
@property(nonatomic,weak)UITextField *textfield;
+(instancetype)keyBoardForTextField:(UITextField*)textField;




- (IBAction)btnClose:(id)sender;


- (IBAction)click1:(id)sender;
- (IBAction)click2:(id)sender;
- (IBAction)click3:(id)sender;
- (IBAction)click4:(id)sender;
- (IBAction)click5:(id)sender;
- (IBAction)click6:(id)sender;
- (IBAction)click7:(id)sender;
- (IBAction)click8:(id)sender;
- (IBAction)click9:(id)sender;
- (IBAction)click0:(id)sender;

@property (weak, nonatomic) IBOutlet UIButton *btnLeft;
- (IBAction)btnLeftClick:(id)sender;

@property (weak, nonatomic) IBOutlet UIButton *btnRight;
- (IBAction)btnRightClick:(id)sender;

- (IBAction)clickDelete:(id)sender;

@property (weak, nonatomic) IBOutlet UIButton *btnConfirm;
- (IBAction)clickConfirm:(id)sender;

-(void)closeKeyBoard;

-(void)filterNumber;

@end









