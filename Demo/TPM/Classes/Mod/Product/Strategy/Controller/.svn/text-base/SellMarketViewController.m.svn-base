-(void)gotoNext{

    [self.navigationController popViewControllerAnimated:NO];
    SellStrategyViewController *controller = [SellStrategyViewController controllerWithEntity:self.sellEntity];
    [[AppUtil findIndex].navigationController  pushViewController:controller animated:YES];

}



@end
