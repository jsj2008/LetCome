#import "SellMarketViewController.h"
#import "ChartBll.h"
#import "STODBManager.h"
#import "SellStrategyViewController.h"

@interface SellMarketViewController ()

@end

@implementation SellMarketViewController
+(instancetype)controllerWithCode:(NSString *)code entity:(SellingListRecordsEntity*)sellEntity{
    SellMarketViewController *c = [self controllerWithCode:code];
    c.sellEntity = sellEntity;
    return c;
}

- (void)viewDidLoad {
    [super viewDidLoad];
    // Do any additional setup after loading the view from its nib.
    [self.nextBtn setTitle:@"平仓策略" forState:UIControlStateNormal];
    self.nextView.hidden = NO;
}

- (void)didReceiveMemoryWarning {
    [super didReceiveMemoryWarning];
    // Dispose of any resources that can be recreated.
}

