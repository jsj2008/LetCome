#import "GetuserUnsignedAgreementEntity.h"

@implementation GetuserUnsignedAgreementEntity
- (NSMutableString *)id_list {
    
    /** 拼接黄金单号 */
    NSMutableString* arrString=[NSMutableString stringWithCapacity:30];
    NSInteger count=[self.res_data count];
    int index=0;
    
    for (GetuserUnsignedAgreementRes_dataEntity* order in self.res_data) {
        [arrString appendString:order.ID];
        
        if(count==1)
        {
            break;
        }
        if(count-1!=index) // 最后一位不加分隔符
            [arrString appendString:@","];
        
        index++;
    }
    _id_list=arrString;
    return _id_list; // 生成黄金单号拼接串
}
@end

@implementation GetuserUnsignedAgreementRes_dataEntity

@end