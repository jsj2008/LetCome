#import "QUEntity.h"
@interface MessageItemEntity : QUEntity
@property(strong,nonatomic)NSMutableArray* records;
@end
@interface MessageItemRecordsEntity : QUEntity
@property(strong,nonatomic)NSString* sender_id;     //送信者ID
@property(strong,nonatomic)NSString* msg_id;      //消息编号
