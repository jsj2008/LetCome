#import "QUEntity.h"

@interface UserDissentOrdersEntity : QUEntity

@property (nonatomic,strong) NSArray *records;

@end


@interface UserDissentOrdersRecordsEntity : QUEntity

/*异议ID*/
@property (nonatomic,copy)NSString *ID;
/*产品id*/
@property (nonatomic,copy)NSString *p_id;
/*产品类型*/
@property (nonatomic,copy)NSString *p_type;
/*异议点 1买入价 2卖出价 3盈亏分配 4交易数量 5亏损赔付 6履约金解冻 7其它*/
@property (nonatomic,copy)NSString *point;
/*异议点翻译文字*/
@property (nonatomic,copy)NSString *point_name;
/*价格*/
@property (nonatomic,copy)NSString *price;
/*异议原因内容*/
@property (nonatomic,copy)NSString *reason;
/*回复内容*/
@property (nonatomic,copy)NSString *comment;
/*创建时间*/
@property (nonatomic,copy)NSString *create_time;
/*更新时间*/
@property (nonatomic,copy)NSString *update_time;
/*回复时间*/
@property (nonatomic,copy)NSString *comment_time;
/*状态 0等待处理 1处理中 2处理完成,无修正 3处理完成，有修正*/
@property (nonatomic,copy)NSString *state;
/*开始时间*/
@property (nonatomic,copy)NSString *start_time;
/*交易单号*/
@property (nonatomic,copy)NSString *pno;
/*点买方向 1：多，2：空，3：双向做多，4：双向做空*/
@property (nonatomic,copy)NSString *operation_direction;
/*产品代码*/
@property (nonatomic,copy)NSString *code;
/*产品名称*/
@property (nonatomic,copy)NSString *stock_name;
/*数量*/
@property (nonatomic,copy)NSString *amount;
/*金额*/
@property (nonatomic,copy)NSString *fund;

@end