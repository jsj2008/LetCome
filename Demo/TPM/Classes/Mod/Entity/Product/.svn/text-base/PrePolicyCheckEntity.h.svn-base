#import <QuickUI/QuickUI.h>

@interface PrePolicyCheckEntity : QUEntity
/*提交结果 'Y':通过/'N:不通过*/
@property(nonatomic,strong)NSString *result;
/*弹框类型 1：去认证弹框 2：风险确认弹框（要用msg_id进行风险确认）3：亏损过多确认框 10：去获取未签署合约列表 20：去设置昵称*/
@property(nonatomic,strong)NSString *reason;
/*确认回传用ID (此参数有可能不存在)*/
@property(nonatomic,strong)NSString *msg_id;
/*提示框信息*/
@property(nonatomic,strong)NSString *msg;
/*类型：3:沪金 4:白银*/
@property(nonatomic,strong)NSString *agreement_type;

@end
