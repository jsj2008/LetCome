#import "QUEntity.h"
#import "ProfitEntity.h"
#define kDirectionMore @"1"
#define kDirectionEmpty @"2"

@interface TodoOrdersEntity : QUEntity

@property (nonatomic,strong) NSArray *records;

@end

@interface TodoOrdersRecordsEntity : QUEntity

/*产品ID*/
@property (nonatomic,copy) NSString *ID;
/*产品类型*/
@property (nonatomic,copy) NSString *p_type;
/*交易单号*/
@property (nonatomic,copy) NSString *pno;
/*方向 1：多 2：空*/
@property (nonatomic,copy) NSString *operation_direction;
/*开始时间*/
@property (nonatomic,copy) NSString *start_time;
/*结束时间*/
@property (nonatomic,copy) NSString *end_time;
/*保证金*/
@property (nonatomic,copy) NSString *bid_bond;
/*开户名*/
@property (nonatomic,copy) NSString *stocker_user_name;
/*实际申购金额*/
@property (nonatomic,copy) NSString *Init_fund;
/*点买金额*/
@property (nonatomic,copy) NSString *fund;
/*状态 默认为-1 正在查询 具体状态请使用产品动态信息接口获取*/
@property (nonatomic,copy) NSString *state;
/*状态翻译文字*/
@property (nonatomic,copy) NSString *state_name;
/*申购数量*/
@property (nonatomic,copy) NSString *amount;
/*产品代码*/
@property (nonatomic,copy) NSString *code;
/*点买方式*/
@property (nonatomic,copy) NSString *buy_way;
/*品种名称*/
@property (nonatomic,copy) NSString *stock_name;
/*投资人id*/
@property (nonatomic,copy) NSString *invester_id;
/*投资人昵称*/
@property (nonatomic,copy) NSString *invester_name;
/*交易市场名*/
@property (nonatomic,copy) NSString *invest_dealer;
/*止赢（盈利目标）*/
@property (nonatomic,copy) NSString *stop_profit_point;
/*止损*/
@property (nonatomic,copy) NSString *stop_loss_point;
/*平仓触发价*/
@property (nonatomic,copy) NSString *sell_start_price;
/*卖出均价*/
@property (nonatomic,copy) NSString *sell_deal_price_avg;
/*开仓触发价*/
@property (nonatomic,copy) NSString *buy_start_price;
/*买入均价*/
@property (nonatomic,copy) NSString *buy_deal_price_avg;
@property (nonatomic,copy) NSString *invest_account;	//投资人账户	string
@property (nonatomic,copy) NSString *invest_location;	//投资人开户地	string
@property (nonatomic,copy) NSString *invest_icon;	//投资人头像地址	string
//以下内容需要从profit接口获取
@property (nonatomic,strong)ProfitRecordsEntity* profitEntity;



@end
