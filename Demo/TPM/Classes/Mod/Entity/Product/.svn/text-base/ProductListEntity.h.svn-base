#import "QUEntity.h"

@interface ProductListEntity : QUEntity

@property (nonatomic,copy)NSMutableArray *records;

@end

@interface ProductListRecordsEntity : QUEntity<NSCoding>

/*产品代码*/
@property (nonatomic,copy)NSString *stock_code;
/*产品名称*/
@property (nonatomic,copy)NSString *stock_name;
/*产品类型*/
@property (nonatomic,copy)NSString *p_type;
/*价格最小波动幅度*/
@property (nonatomic,copy)NSString *price_min_vol;
/*盈利目标最小波动幅度（>=1为固定值 <1为百分比）*/
@property (nonatomic,copy)NSString *profit_min_vol;
/*盈利目标最大值（>=1为固定值 <1为百分比）*/
@property (nonatomic,copy)NSString *profit_max;

@property (nonatomic,copy)NSArray *profit_array;

@property (nonatomic,copy)NSString *profit_default;

@property (nonatomic,copy)NSString *max_earnest;//保证金波动容忍乘数
@property (nonatomic,copy)NSString *max_trigger_percent;//	触发价上限与昨收价的差额百分比	float
@property (nonatomic,copy)NSString *min_trigger_percent;//	触发价下限与昨收价的差额百分比	float

@property (nonatomic,copy)NSString *icon;//	图标地址	string
@property (nonatomic,copy)NSString *decimal;//	行情显示的小数位数 0:无小数位数 n:n位小数	int



@end
