#import <QuickUI/QuickUI.h>

@interface SchemeDetailEntity : QUEntity
@property(nonatomic,strong)NSString *fund;
@property(nonatomic,strong)NSString *bid_bond;
@property(nonatomic,strong)NSString *break_cost;
@property(nonatomic,strong)NSString *fee;
/*持股时长*/
@property(nonatomic,strong)NSString *hold_time;
/*委买时间*/
@property(nonatomic,strong)NSString *buy_time;
/*市价买入*/
@property(nonatomic,strong)NSString *buy_way;
/*委买次数*/
@property(nonatomic,strong)NSString *buy_times;
/*通知日*/
@property(nonatomic,strong)NSString *apply_sell_date;
/*通知时间*/
@property(nonatomic,strong)NSString *apply_sell_time;
/*委卖时间*/
@property(nonatomic,strong)NSString *sell_time;
/*委卖方式*/
@property(nonatomic,strong)NSString *sell_way;
/*委卖次数*/
@property(nonatomic,strong)NSString *sell_times;
/*成本补偿(达到目标)*/
@property(nonatomic,strong)NSString *success_cost;
/*享有盈利（达到 目标)*/
@property(nonatomic,strong)NSString *success_profit;
/*赔付亏损（达到目标)*/
@property(nonatomic,strong)NSString *success_loss;
/*成本补偿(未达到目标)*/
@property(nonatomic,strong)NSString *fail_cost;
/*享有盈利（未达到 目标)*/
@property(nonatomic,strong)NSString *fail_profit;
/*赔付亏损（未达到目标)*/
@property(nonatomic,strong)NSString *fail_loss;
/*盈利目标*/
@property(nonatomic,strong)NSString *stop_profit_point;

@end
