#import <QuickUI/QuickUI.h>
#import "ProfitEntity.h"

@interface BuyingListEntity : QUEntity

@property (nonatomic,strong) NSArray *records;

@end

@interface BuyingListRecordsEntity : QUEntity
/*交易ID*/
@property(nonatomic,strong)NSString *ID;
/*交易单号*/
@property(nonatomic,strong)NSString *pno;
/*点买方向 1:多 2:空 3:双向多 4:双向空*/
@property(nonatomic,strong)NSString *operation_direction;
/*开始时间*/
@property(nonatomic,strong)NSString *start_time;
/*结束时间*/
@property(nonatomic,strong)NSString *end_time;
/*保证金*/
@property(nonatomic,strong)NSString *bid_bond;
/*开户名*/
@property(nonatomic,strong)NSString *stocker_user_name;
/*实际申购金额*/
@property(nonatomic,strong)NSString *Init_fund;
/*剩余申购金额*/
@property(nonatomic,strong)NSString *left_fund;
/*投资单元*/
@property(nonatomic,strong)NSString *fund;
/*状态 默认为-1 正在查询 具体状态请使用产品动态信息接口获取*/
@property(nonatomic,strong)NSString *state;
/*状态翻译文字*/
@property(nonatomic,strong)NSString *state_name;
/*申购数量*/
@property(nonatomic,strong)NSString *amount;
/*产品代码*/
@property(nonatomic,strong)NSString *code;
/*点买方式*/
@property(nonatomic,strong)NSString *buy_way;
/*股票名称*/
@property(nonatomic,strong)NSString *stock_name;
/*投资人Id*/
@property(nonatomic,strong)NSString *investor_id;
/*投资人账户*/
@property(nonatomic,strong)NSString *invest_account;
/*投资人代号*/
@property(nonatomic,strong)NSString *dealer_replace;
/*投资人名*/
@property(nonatomic,strong)NSString *invester_name;
/*交易市场*/
@property(nonatomic,strong)NSString *invest_dealer;
/*投资人开户营业部*/
@property(nonatomic,strong)NSString *invest_open_dealer;
/*投资人开户地*/
@property(nonatomic,strong)NSString *invest_location;
/*投资人头像地址*/
@property(nonatomic,strong)NSString *invest_icon;
/*止赢（盈利目标）*/
@property(nonatomic,strong)NSString *stop_profit_point;
/*止损*/
@property(nonatomic,strong)NSString *stop_loss_point;
/*开仓触发价*/
@property(nonatomic,strong)NSString *start_price;
/*买入均价*/
@property(nonatomic,strong)NSString *buy_deal_price_avg;
/*使用率*/
@property(nonatomic,strong)NSString *use_rate;
/*申购次数*/
@property(nonatomic,strong)NSString *times;
/*最大申购次数*/
@property(nonatomic,strong)NSString *max_times;

@property (nonatomic, strong) ProfitRecordsEntity *profitEntity;

@end