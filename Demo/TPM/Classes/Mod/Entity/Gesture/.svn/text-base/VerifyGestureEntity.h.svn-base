#import "QUEntity.h"

@interface VerifyGestureEntity : QUEntity

/*0:登录成功 79001:失败 79002:失败次数过多，需要验证码 79003:失败次数过多，帐户被锁定*/
@property (nonatomic,copy)NSString *error_code;
/*用户UID*/
@property (nonatomic,copy)NSString *uid;
/*会话ID*/
@property (nonatomic,copy)NSString *session_id;
/*签名加密串*/
@property (nonatomic,copy)NSString *encryptedKey;
/*密码输入错误次数*/
@property (nonatomic,copy)NSString *failed_times;

@end
