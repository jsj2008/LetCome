<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/settlement_list_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/item_bar_bg_selctor"
    android:orientation="vertical" >
    <include 
        android:id="@+id/view_line_top"
	    layout="@layout/horizontal_line_separate" />
    
	<View 
	    android:id="@+id/view_top"
    	android:layout_width="match_parent"
    	android:layout_height="@dimen/margin_xhdpi_20"
    	android:background="@color/product_bg" >
	</View>

	<include 
	    layout="@layout/horizontal_line_separate" />
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/margin_xhdpi_15"
        android:paddingRight="@dimen/font_size_xhdpi_15"
        android:paddingTop="@dimen/margin_xhdpi_10"
        android:paddingBottom="@dimen/margin_xhdpi_13"
        android:orientation="vertical"
        android:background="@color/color_fafafa">
         <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView 
                android:id="@+id/tv_pno_show"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settlement_list_pno"
                android:layout_alignParentLeft="true"
                android:textAppearance="@style/font_999999_12"/>
            <TextView 
                android:id="@+id/tv_pno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/tv_pno_show"
                android:textAppearance="@style/font_999999_12"
                android:layout_marginLeft="@dimen/margin_xhdpi_5"/>
            <TextView 
                android:id="@+id/tv_create_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textAppearance="@style/font_999999_12"/>
         </RelativeLayout>
         <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_xhdpi_13">
            <TextView 
                android:id="@+id/tv_intestor_show"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settlement_list_investor"
                android:layout_alignParentLeft="true"
                android:drawableRight="@drawable/item_cert"
                android:textAppearance="@style/font_999999_12"
                android:drawablePadding="@dimen/margin_xhdpi_5"/>
            <TextView 
                android:id="@+id/tv_deal_replace"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="@style/font_999999_12"
                android:layout_toRightOf="@+id/tv_intestor_show"
                android:layout_marginLeft="@dimen/margin_xhdpi_5"/>
            <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_alignParentRight="true">
                <TextView 
                    android:id="@+id/tv_fund"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="@style/font_999999_12"/>

                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_999999"
                    android:layout_marginLeft="@dimen/margin_xhdpi_8"
                    android:layout_marginRight="@dimen/margin_xhdpi_8"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="@dimen/margin_xhdpi_2"
                    android:layout_marginBottom="@dimen/margin_xhdpi_2" />

                <TextView 
                    android:id="@+id/tv_stock_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="@style/font_999999_12"/>
            </LinearLayout>
         </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/tab_cur_selected"
        android:paddingBottom="@dimen/margin_xhdpi_20" >
        <TextView 
            android:id="@+id/tv_invest_profit_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/settlement_list_invest_profit"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/margin_xhdpi_35"
            android:textAppearance="@style/font_white_12"/>
        <TextView 
            android:id="@+id/tv_profit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/tv_invest_profit_show"
            android:textAppearance="@style/font_white_25"
            android:layout_marginTop="@dimen/margin_xhdpi_10"/>
        <ImageView 
            android:paddingTop="@dimen/margin_xhdpi_20"
            android:paddingLeft="@dimen/margin_xhdpi_20"
            android:paddingBottom="@dimen/margin_xhdpi_20"
            android:paddingRight="@dimen/margin_xhdpi_15"
            android:id="@+id/img_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/item_detail"
            android:layout_alignParentRight="true"/>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/margin_xhdpi_20"
            android:layout_below="@+id/tv_profit"
            android:paddingLeft="@dimen/margin_xhdpi_15"
            android:paddingRight="@dimen/margin_xhdpi_15">
            <LinearLayout 
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">
                <TextView 
                    android:id="@+id/tv_buy_price_show"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/settlement_list_buy_price"
                    android:textAppearance="@style/font_white_12"/>
                <TextView 
                    android:id="@+id/tv_buy_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_xhdpi_8"
                    android:textAppearance="@style/font_white_12"/>
            </LinearLayout>
            <LinearLayout 
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">
                <TextView 
                    android:id="@+id/tv_sell_price_show"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/settlement_list_sell_price"
                    android:textAppearance="@style/font_white_12"/>
                <TextView 
                    android:id="@+id/tv_sell_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_xhdpi_8"
                    android:textAppearance="@style/font_white_12"/>
            </LinearLayout>
            <LinearLayout 
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">
                <TextView 
                    android:id="@+id/tv_amount_show"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/settlement_list_amount"
                    android:textAppearance="@style/font_white_12"/>
                <TextView 
                    android:id="@+id/tv_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_xhdpi_8"
                    android:textAppearance="@style/font_white_12"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_color"
        android:paddingLeft="@dimen/margin_xhdpi_15"
        android:paddingRight="@dimen/margin_xhdpi_15"
        android:paddingBottom="@dimen/margin_xhdpi_15">
        <TextView
            android:id="@+id/tv_tactics_cooperate_profit_show" 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="@string/settlement_list_tactics_cooperate_profit"
            android:layout_marginTop="@dimen/margin_xhdpi_50"
            android:textAppearance="@style/font_999999_12"/>
        <TextView
            android:id="@+id/tv_user_profit" 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/tv_tactics_cooperate_profit_show"
            android:textSize="@dimen/font_size_xhdpi_25"
            android:layout_marginTop="@dimen/margin_xhdpi_10"
            android:textColor="@color/color_999999"/>
        <LinearLayout 
            android:id="@+id/view_dissent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@color/transparent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/margin_xhdpi_15">
            <TextView
            android:id="@+id/tv_dissent_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/transparent"
            android:textAppearance="@style/font_cccccc_12"/>
            <TextView
            android:id="@+id/tv_dissent_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_xhdpi_5"
            android:textColor="@color/tab_cur_selected"
            android:textSize="@dimen/font_size_xhdpi_12"/>
        </LinearLayout>
        <TableLayout 
            android:id="@+id/ly_table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="*"
            android:shrinkColumns="*"
            android:layout_marginTop="@dimen/margin_xhdpi_20"
            android:layout_below="@+id/tv_user_profit">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:gravity="center"
                    android:text="@string/settlement_list_break_cost"
                    android:background="@color/color_ebebeb"
                    android:textAppearance="@style/font_999999_12"/>
                <TextView 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:gravity="center"
                    android:text="@string/settlement_list_share_profit"
                    android:layout_marginLeft="@dimen/vertical_line_separate_width"
                    android:background="@color/color_ebebeb"
                    android:textAppearance="@style/font_999999_12"/>
                <TextView 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:gravity="center"
                    android:text="@string/settlement_list_stop_profit_point"
                    android:layout_marginLeft="@dimen/vertical_line_separate_width"
                    android:background="@color/color_ebebeb"
                    android:textAppearance="@style/font_999999_12"/>
            </TableRow>
            <TableRow 
                android:layout_marginTop="@dimen/horizontal_line_separate_height"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/margin_xhdpi_30">
                <LinearLayout 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:background="@color/color_fafafa">
                    <TextView 
	                    android:id="@+id/tv_break_cost"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:textSize="@dimen/font_size_xhdpi_12"/>
                    <TextView 
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:text="@string/money_unit"
	                    android:textAppearance="@style/font_999999_12"/>
                </LinearLayout>
                <LinearLayout 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginLeft="@dimen/vertical_line_separate_width"
                    android:background="@color/color_fafafa">
                    <TextView 
	                    android:id="@+id/tv_share_profit"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:textSize="@dimen/font_size_xhdpi_12"/>
                    <TextView 
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:text="@string/money_unit"
	                    android:textAppearance="@style/font_999999_12"/>
                </LinearLayout>
                <LinearLayout 
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_xhdpi_30"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginLeft="@dimen/vertical_line_separate_width"
                    android:background="@color/color_fafafa">
                      <TextView 
		                    android:id="@+id/tv_stop_profit_point"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:textSize="@dimen/font_size_xhdpi_12"/>
                      <TextView 
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:text="@string/money_unit"
		                    android:textAppearance="@style/font_999999_12"/>
                </LinearLayout>       
            </TableRow>
        </TableLayout>
        <LinearLayout
            android:id="@+id/view_pay_commit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_xhdpi_40"
            android:orientation="horizontal"
            android:layout_below="@+id/ly_table"
            android:layout_marginTop="@dimen/margin_xhdpi_25" 
            android:gravity="center"
            android:layout_marginBottom="@dimen/margin_xhdpi_15">
            <TextView 
                android:id="@+id/tv_state_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableRight="@drawable/item_time2_w"
                android:textSize="@dimen/font_size_xhdpi_16"
                android:drawablePadding="@dimen/margin_xhdpi_5"
                android:layout_marginRight="@dimen/margin_xhdpi_5"/>
            <TextView 
                android:id="@+id/tv_countdown_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_size_xhdpi_16"
                android:textColor="@color/white_color"/>
        </LinearLayout>
    </RelativeLayout>
    <include 
        android:id="@+id/view_line_bottom"
	    layout="@layout/horizontal_line_separate" />
</LinearLayout>